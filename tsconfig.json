{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "outDir": "./dist",
    "alwaysStrict": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "@util/*": ["util/*"],
      "@database/*": ["database/*"],
      "@bot/*": ["bot/*"]
    }
  },
  "include": [
    "./src/"
  ],
  "exclude": [
    "node_modules"
  ]
}
