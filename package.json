{
  "name": "typehypebot",
  "author": "Sam Mckenzie",
  "main": "start.js",
  "version": "0.9.0",
  "description": "A TypeScript framework for creating bots that purchase items from certain websites",
  "scripts": {
    "build": "tsc",
    "bas": "echo 'DEVELOPMENT MODE'; tsc; NODE_ENV=development node dist/App",
    "start": "echo 'PRODUCTION MODE'; NODE_ENV=production node dist/App",
    "startdev": "echo 'DEVELOPMENT MODE'; NODE_ENV=development node dist/App",
    "lint": "./node_modules/.bin/tslint --project tsconfig.json --force",
    "lintfix": "./node_modules/.bin/tslint --project tsconfig.json --fix --force",
    "gensecret": "node -e \"console.log(crypto.randomBytes(64).toString('hex'))\""
  },
  "dependencies": {
    "anti-captcha": "0.0.3",
    "cheerio": "^1.0.0-rc.2",
    "country-data": "0.0.31",
    "crypto": "0.0.3",
    "digitalocean": "^0.11.0",
    "discord.js": "^11.4.2",
    "faker": "^4.1.0",
    "http-status-codes": "^1.3.0",
    "ip": "^1.1.5",
    "moment": "^2.22.2",
    "nexmo": "^2.3.2",
    "node": "^10.10.0",
    "node-ssh": "^5.1.2",
    "opn": "^5.3.0",
    "puppeteer": "^1.8.0",
    "querystring": "^0.2.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "slackbots": "^1.1.0",
    "ssh-keygen": "^0.4.2",
    "twilio": "^3.19.2",
    "twitter": "^1.7.1",
    "us-state-codes": "^1.1.2",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.9",
    "@types/faker": "^4.1.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^8.10.29",
    "@types/opn": "^5.1.0",
    "@types/puppeteer": "^1.6.3",
    "@types/request-promise-native": "^1.0.15",
    "@types/ws": "^5.1.2",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^2.9.2"
  }
}
